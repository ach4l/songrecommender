	<li id="fullcomment-73016244552">
		<a name="comment-73016244552"></a>
		
		<div class="com-normal">
			
<div id="rating-holder-73016244552">
	<div class="numb-holder">
		<span id="com-rating-73016244552">
			<strong class="numb" id='numb-rating-73016244552'>+1</strong>
		</span>
	
				
		<div style="display: none; text-align: center;" id="com-whorated-73016244552" class="com-whorated">
			<span class="processing"></span>
		</div>

