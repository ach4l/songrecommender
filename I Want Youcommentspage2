	<li id="fullcomment-73016016294">
		<a name="comment-73016016294"></a>
		
		<div class="com-normal">
			
<div id="rating-holder-73016016294">
	<div class="numb-holder">
		<span id="com-rating-73016016294">
			<strong class="numb" id='numb-rating-73016016294'>0</strong>
		</span>
	
				
		<div style="display: none; text-align: center;" id="com-whorated-73016016294" class="com-whorated">
			<span class="processing"></span>
		</div>

		<div id="processing-73016016294" style="text-align: center; padding: 8px 8px 0px 12px; display: none;">
			<span class="processing"></span>
		</div>
