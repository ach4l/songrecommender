<!-- BEGIN JS TAG - SM ROS ATF < - DO NOT MODIFY -->
<SCRIPT SRC="http://ib.adnxs.com/ttj?id=5951356&size=300x250" TYPE="text/javascript"></SCRIPT>
<!-- END TAG --> <br/><div class="block" style="padding-top: 10px; margin-top: 0px; border-top: 1px solid #DEDEDE;">

	<div id="rate-widget-57341" class="rating-widget" itemprop="aggregateRating" itemscope itemtype="http://schema.org/AggregateRating">
		<div id="rating-header">
			Rate These Lyrics
		</div>

		<meta itemprop="ratingValue" content="5" />
	    <meta itemprop="worstRating" content="1"/>
	    <meta itemprop="bestRating" content="5" />
	    <meta itemprop="reviewCount" content="3" />

		<span>
			<a id="star-1" href="#" class="star-1 rate-star"></a>
			<a id="star-2" href="#" class="star-2 rate-star"></a>
			<a id="star-3" href="#" class="star-3 rate-star"></a>
			<a id="star-4" href="#" class="star-4 rate-star"></a>
			<a id="star-5" href="#" class="star-5 rate-star"></a>
		</span>

		<div class="total_votes" data-totalvotes="3 Votes">
			3 Votes		</div>
	</div>
</div>

<script type="text/javascript">
	$(document).ready(function() {
		var url = window.location.href;
		if (/\/$/.test(url) == false) {
	   		url = url + "/";
		}

		var total_votes = '3';
		var average 	= '5';
		setRating('.rating-widget');

		var loggedin 	= 'false';

		$('.rate-star').hover(
            // Handles the mouseover
            function() {
                $(this).prevAll().andSelf().addClass('rate-star-hover');
                $(this).nextAll().removeClass('rate-star-vote'); 

                var rating_fyi = $(this).attr('id');
                switch(rating_fyi) {
                	case 'star-1':
                		rating_fyi = "poor";
                	break;

                	case 'star-2':
                		rating_fyi = "not so good";
                	break;

                	case 'star-3':
                		rating_fyi = "average";
                	break;

                	case 'star-4':
                		rating_fyi = "good";
                	break;

                	case 'star-5':
                		rating_fyi = "excellent";
                	break;
                }
                $('.total_votes').html(rating_fyi);
            },
            // Handles the mouseout
            function() {
                $(this).prevAll().andSelf().removeClass('rate-star-hover');
                //setRating($(this).parent());
                $('.total_votes').html($('.total_votes').data('totalvotes'));
            }
        );

        $('.rate-star').bind('click', function(e) {
            var star = this;
            var lid = $(this).closest("div").attr("id").substring(12);
            
            var clicked_data = {
            	command    	: "songRatingSet",
            	lyric_id	: lid,
                clicked_on 	: $(star).attr('class'),
                clicked_id 	: $(star).attr('id')
            };

            $('.rate-star').removeClass('rate-star-full');

            $.ajax({
				type: "POST", 
				url: url, 
				data: clicked_data,
				dataType: "json",
				success: function(result){
					//$('.rating-widget').data('fsr', result);
					total_votes			= result['total_votes'];
					if(typeof(result['user_value']) != "undefined" && result['user_value'] !== null) {
						average 			= result['user_value'];
					} else {
						average 			= result['average'];
					}
						
					setRating('.rating-widget');
				},
				complete: function(result, success) {
					console.log(result, success);
				}
			});

	        return false;
        }); 

        function setRating(widget) {
        	$(widget).find('.star-' + average).prevAll().andSelf().addClass('rate-star-full');  
    		$(widget).find('.star-' + average).nextAll().removeClass('rate-star-full');  

    		if(total_votes === "1") {
    			total_votes = "1 Vote";
    		} else {
    			total_votes = total_votes + " Votes";
    		}

    		$('.total_votes').data('totalvotes', total_votes);
        	$('.total_votes').html(total_votes);
        	return false;
        }
        
    
	});
</script>
