<script type="text/javascript">
	$(document).ready(function() {
		var url = window.location.href;
		if (/\/$/.test(url) == false) {
	   		url = url + "/";
		}

		var total_votes = '7';
		var average 	= '3';
		setRating('.rating-widget');

		var loggedin 	= 'false';

		$('.rate-star').hover(
            // Handles the mouseover
            function() {
                $(this).prevAll().andSelf().addClass('rate-star-hover');
                $(this).nextAll().removeClass('rate-star-vote'); 

                var rating_fyi = $(this).attr('id');
                switch(rating_fyi) {
                	case 'star-1':
                		rating_fyi = "poor";
                	break;

                	case 'star-2':
                		rating_fyi = "not so good";
                	break;

                	case 'star-3':
                		rating_fyi = "average";
                	break;

                	case 'star-4':
                		rating_fyi = "good";
                	break;

                	case 'star-5':
                		rating_fyi = "excellent";
                	break;
                }
                $('.total_votes').html(rating_fyi);
            },
            // Handles the mouseout
            function() {
                $(this).prevAll().andSelf().removeClass('rate-star-hover');
                //setRating($(this).parent());
                $('.total_votes').html($('.total_votes').data('totalvotes'));
            }
        );

        $('.rate-star').bind('click', function(e) {
            var star = this;
            var lid = $(this).closest("div").attr("id").substring(12);
            
            var clicked_data = {
            	command    	: "songRatingSet",
            	lyric_id	: lid,
                clicked_on 	: $(star).attr('class'),
                clicked_id 	: $(star).attr('id')
            };

            $('.rate-star').removeClass('rate-star-full');

            $.ajax({
				type: "POST", 
				url: url, 
				data: clicked_data,
				dataType: "json",
				success: function(result){
					//$('.rating-widget').data('fsr', result);
					total_votes			= result['total_votes'];
					if(typeof(result['user_value']) != "undefined" && result['user_value'] !== null) {
						average 			= result['user_value'];
					} else {
						average 			= result['average'];
					}
						
					setRating('.rating-widget');
				},
				complete: function(result, success) {
					console.log(result, success);
				}
			});

	        return false;
        }); 

        function setRating(widget) {
        	$(widget).find('.star-' + average).prevAll().andSelf().addClass('rate-star-full');  
    		$(widget).find('.star-' + average).nextAll().removeClass('rate-star-full');  

    		if(total_votes === "1") {
    			total_votes = "1 Vote";
    		} else {
    			total_votes = total_votes + " Votes";
    		}

    		$('.total_votes').data('totalvotes', total_votes);
        	$('.total_votes').html(total_votes);
        	return false;
        }
        
    
	});
</script>
