	<li id="fullcomment-73016134081">
		<a name="comment-73016134081"></a>
		
		<div class="com-normal">
			
<div id="rating-holder-73016134081">
	<div class="numb-holder">
		<span id="com-rating-73016134081">
			<strong class="numb" id='numb-rating-73016134081'>0</strong>
		</span>
	
				
		<div style="display: none; text-align: center;" id="com-whorated-73016134081" class="com-whorated">
			<span class="processing"></span>
		</div>

		<div id="processing-73016134081" style="text-align: center; padding: 8px 8px 0px 12px; display: none;">
