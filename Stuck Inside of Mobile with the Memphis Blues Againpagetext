	<div class="answer-holder" id="fullcomment-73016313091">
		<a name="comment-73016313091"></a>
		
<div id="rating-holder-73016313091">
	<div class="numb-holder">
		<span id="com-rating-73016313091">
			<strong class="numb" id='numb-rating-73016313091'>+1</strong>
		</span>
	
				
		<div style="display: none; text-align: center;" id="com-whorated-73016313091" class="com-whorated">
			<span class="processing"></span>
		</div>

		<div id="processing-73016313091" style="text-align: center; padding: 8px 8px 0px 12px; display: none;">
