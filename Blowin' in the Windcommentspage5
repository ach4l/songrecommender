	<li id="fullcomment-73015235839">
		<a name="comment-73015235839"></a>
		
		<div class="com-normal">
			
<div id="rating-holder-73015235839">
	<div class="numb-holder">
		<span id="com-rating-73015235839">
			<strong class="numb" id='numb-rating-73015235839'>0</strong>
		</span>
	
				
		<div style="display: none; text-align: center;" id="com-whorated-73015235839" class="com-whorated">
			<span class="processing"></span>
		</div>

		<div id="processing-73015235839" style="text-align: center; padding: 8px 8px 0px 12px; display: none;">
			<span class="processing"></span>
