	<li id="fullcomment-73014709767">
		<a name="comment-73014709767"></a>
		
		<div class="com-normal">
			
<div id="rating-holder-73014709767">
	<div class="numb-holder">
		<span id="com-rating-73014709767">
			<strong class="numb" id='numb-rating-73014709767'>+2</strong>
		</span>
	
				
		<div style="display: none; text-align: center;" id="com-whorated-73014709767" class="com-whorated">
			<span class="processing"></span>
		</div>

		<div id="processing-73014709767" style="text-align: center; padding: 8px 8px 0px 12px; display: none;">
