	<li id="fullcomment-73016099839">
		<a name="comment-73016099839"></a>
		
		<div class="com-normal">
			
<div id="rating-holder-73016099839">
	<div class="numb-holder">
		<span id="com-rating-73016099839">
			<strong class="numb" id='numb-rating-73016099839'>+3</strong>
		</span>
	
				
		<div style="display: none; text-align: center;" id="com-whorated-73016099839" class="com-whorated">
			<span class="processing"></span>
