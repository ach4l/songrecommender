	<div class="answer-holder" id="fullcomment-73016136236">
		<a name="comment-73016136236"></a>
		
<div id="rating-holder-73016136236">
	<div class="numb-holder">
		<span id="com-rating-73016136236">
			<strong class="numb" id="numb-rating-73016136236">0</strong>
		</span>
	
				
		<div style="display: none; text-align: center;" id="com-whorated-73016136236" class="com-whorated">
			<span class="processing"></span>
